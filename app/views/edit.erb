<form method="post" action="/posts/<%= @post.id %>">
  <input id="hidden" type="hidden" name="_method" value="patch">
  Post Name: <input id="name" type="text" name="name" value="<%= @post.name %>">
  Post Content: <input id="content" type="text" name="content" value="<%= @post.content %>">
  <input id="submit" type="submit" value="save">
</form>
